/*
Faça um Programa que peça dois números e imprima a soma.
*/
programa {
  funcao inicio() {
    inteiro n1, n2, resultado

    escreva ("informe um numero: ")
    leia(n1)

    escreva ("informe outro numero: ")
    leia(n2)

    resultado = n1 + n2

    escreva("a soma dos numeros é igual a: " + resultado)
  }
}
